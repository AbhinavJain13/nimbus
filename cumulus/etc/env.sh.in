    PYTHONPATH=@@INSTALLDIR@@
else
    PYTHONPATH=@@INSTALLDIR@@:${PYTHONPATH}
fi
export PYTHONPATH

CUMULUS_HOME=@@INSTALLDIR@@
export CUMULUS_HOME

CUMULUS_AUTHZ_DDL=@@INSTALLDIR@@/etc/acl.sql
